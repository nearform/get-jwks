  },
  "homepage": "https://github.com/nearform/get-jwks#readme",
  "dependencies": {
    "jwk-to-pem": "^2.0.4",
    "node-fetch": "^2.6.1",
    "tiny-lru": "^7.0.6"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "fast-jwt": "^1.1.2",
    "fastify": "^3.12.0",
    "fastify-jwt": "^2.3.0",
